<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :song_genre_id %>
  <%= f.text_field :genre_name, id: "song_genre_id", list: "genres_autocomplete" %>
  <datalist id="genres_autocomplete">
    <% Genre.all.each do |g| %>
      <option value="<%= g.name%>">
    <% end %>
  </datalist>
  <br>
  <input id="song_notes_1" name="song[song_notes][]">
  <br>
  <input id="song_notes_2" name="song[song_notes][]">
  <br>
  <input id="song_notes_3" name="song[song_notes][]">
  <%= f.submit %>
<% end %>


